<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context = "http://www.springframework.org/schema/context"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
         http://www.springframework.org/schema/context
         http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <context:annotation-config/>

    <!-- Zdefiniowanie fasolki samochód wraz z wymaganymi argumentami -->
    <bean id="reaktorjądrowy" class = "adnotacje.qualifier.komponentyelektrowni.ReaktorJądrowy"/>
    <bean id="reaktorchemiczny" class="adnotacje.qualifier.komponentyelektrowni.ReaktorChemiczny"/>
    <bean id="elektrownia" class="adnotacje.qualifier.Elektrownia"/>
</beans>