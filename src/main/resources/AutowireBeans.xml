<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context.xsd">
    <context:annotation-config/>
    <!-- Zdefiniowanie fasolki klawiatura bezarumentowo -->
    <bean id="klawiatura" class="adnotacje.autowired.Klawiatura"/>
    <!-- Zdefiniowanie fasolki przycisk -->
    <bean id="przycisk" class="adnotacje.autowired.Przycisk"/>
    <!-- Zdefiniowanie fasolki dioda -->
    <bean id="dioda" class="adnotacje.autowired.Dioda"/>
    <!-- Zdefiniowanie fasolki nóżka -->
    <bean id="nóżka" class="adnotacje.autowired.Nóżka"/>
    <!-- Zdefiniowanie fasolki monitor-->
    <bean id="monitor" class="adnotacje.autowired.Monitor"/>
    <!-- Zdefiniowanie fasolki komputer -->
    <bean id="komputer" class="adnotacje.autowired.Komputer"/>
</beans>